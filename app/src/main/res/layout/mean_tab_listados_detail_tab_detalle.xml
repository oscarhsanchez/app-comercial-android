                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/med_dgrales_traficoVehicular"
                        style="@style/basic_style"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:scaleType="fitStart"
                        android:layout_column="2"
                        android:layout_marginLeft="15dp" />
                </TableRow>

                <TableRow
                    style="@style/wrap_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp">

                    <TextView
                        style="@style/basic_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:background="@null"
                        android:text="@string/lbl_medios_datosG_traficoTranseuntes"
                        android:textColor="@color/orangeGpo"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/med_dgrales_traficoTranseuntes"
                        style="@style/basic_style"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_column="2"
                        android:layout_marginLeft="15dp"
                        android:scaleType="fitStart"/>
                </TableRow>

                <TableRow
                    style="@style/wrap_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp">

                    <TextView
                        style="@style/basic_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:background="@null"
                        android:text="@string/lbl_medios_datosG_NSE"
                        android:textColor="@color/orangeGpo"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/med_dgrales_NSE"
                        style="@style/basic_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="2"
                        android:layout_marginLeft="15dp"
                        android:background="@null"
                        android:text="New Text"
                        android:textColor="@color/text_datos_generales_center"
                        android:textSize="17sp" />
                </TableRow>

                <TableRow
                    style="@style/wrap_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp">

                    <TextView
                        style="@style/basic_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:background="@null"
                        android:text="@string/lbl_medios_datosG_categoria"
                        android:textColor="@color/orangeGpo"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/med_dgrales_categoria"
                        style="@style/basic_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="2"
                        android:layout_marginLeft="15dp"
                        android:background="@null"
                        android:text="New Text"
                        android:textColor="@color/text_datos_generales_center"
                        android:textSize="17sp" />
                </TableRow>

                <TableRow
                    style="@style/wrap_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp">

                    <TextView
                        style="@style/basic_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:background="@null"
                        android:text="@string/lbl_medios_datosG_fechaInstalacion"
                        android:textColor="@color/orangeGpo"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/med_dgrales_fechaInstalacion"
                        style="@style/basic_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="2"
                        android:layout_marginLeft="15dp"
                        android:background="@null"
                        android:text="New Text"
                        android:textColor="@color/text_datos_generales_center"
                        android:textSize="17sp" />
                </TableRow>



            </TableLayout>

        </LinearLayout>


    </LinearLayout>
